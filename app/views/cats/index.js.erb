function replaceKittens (innerHTML) {
  var kittens = $('#kittens');
  $('.preloader-wrapper').removeClass('active');
  $(innerHTML).appendTo(kittens).hide().fadeIn(400);
  objectFitPolyfill();
}

<% if @kittens.present? %>
replaceKittens("<%= j(render 'page_items')%>");
$('#div_next_link').html("<%= j(render 'shared/next_link') %>");
<% else %>
replaceKittens("<div class='col s12 center-align'><span class='empty-message'><%= t('index.no_kittens_found') %>!</span></div>");
<% end %>

