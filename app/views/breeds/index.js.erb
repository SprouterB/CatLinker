function replaceBreeds (innerHTML) {
  var breeds = $('#breeds');
  $('.preloader-wrapper').removeClass('active');
  if (window.active_filter) {
    $(breeds).html('');
  }
  $(innerHTML).appendTo(breeds).hide().fadeIn(400);
}

replaceBreeds("<%= j(render 'page_items')%>");
$('#div_next_link').html("<%= j(render 'shared/next_link') %>");

